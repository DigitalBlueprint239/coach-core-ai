# Coach Core AI Environment Configuration
# Copy this file to .env.local and fill in your values

# Application Configuration
REACT_APP_VERSION=0.1.0
REACT_APP_BUILD_NUMBER=1
REACT_APP_ENVIRONMENT=development

# Firebase Configuration
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef
REACT_APP_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-domain.com/api
REACT_APP_AI_SERVICE_URL=https://your-ai-service.com

# AI Configuration
REACT_APP_OPENAI_API_KEY=your-openai-api-key
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_AI_ASSISTANT=true
REACT_APP_AI_PROXY_TOKEN=your-ai-proxy-token

# Supabase Configuration (Alternative Database)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_HSTS=true
REACT_APP_VAPID_KEY=your-vapid-key

# Third-party Integrations
REACT_APP_ENABLE_HUDL_INTEGRATION=false
REACT_APP_HUDL_CLIENT_ID=your-hudl-client-id
REACT_APP_HUDL_CLIENT_SECRET=your-hudl-client-secret
REACT_APP_HUDL_REDIRECT_URI=https://your-app.com/auth/hudl/callback

# Payment Integration
REACT_APP_ENABLE_STRIPE_INTEGRATION=false
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Error Reporting
REACT_APP_ERROR_REPORTING_ENDPOINT=https://your-error-reporting-service.com
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Database Configuration
REACT_APP_DATABASE_PROVIDER=firebase

# Development Configuration
REACT_APP_USE_EMULATOR=false 