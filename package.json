{
  "name": "coach-core-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "validate": "node scripts/validate-build.js",
    "validate:comprehensive": "node scripts/comprehensive-validation.js",
    "validate:quick": "npm run type-check && npm run lint",
    "prebuild": "npm run validate:comprehensive",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "audit": "npm audit --audit-level=moderate",
    "deps:check": "npm ls --depth=0",
    "deps:outdated": "npm outdated",
    "security:scan": "npm audit && npm run deps:check",
    "build:safe": "npm run validate:comprehensive && npm run build",
    "ci:validate": "npm run validate:comprehensive && npm run test"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
